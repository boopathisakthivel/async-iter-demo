{
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015-node": "^6.1.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-plugin-transform-async-generator-functions": "^6.17.0"
