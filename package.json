    "babel-register": "^6.9.0",
    "babel-polyfill": "^6.16.0",
    "mocha": "^3.1.0"
  },
  "scripts": {
    "b": "babel-node",
    "test": "mocha --ui qunit --require babel-polyfill --compilers js:babel-register"
  },
  "babel": {
    "presets": [
      "es2015-node",
      "es2016",
      "es2017"
    ],
    "plugins": [
      "transform-async-generator-functions"
    ]
  },
